							rgba: root.ch1_color
						Rectangle:
							pos: self.pos
							size: self.size
							
					Label:
						id: lbl_4
						text: root.ch1_rating
						outline_width: 2
						#outline_color: (0,0,0)
						#color: (0,0,0)
						padding: 1,1
						font_size: 20
				
				BoxLayout:
