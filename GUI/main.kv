<Button>:
	font_size: 25
		
<Channel_1>:	
	BoxLayout:
		orientation: 'vertical'
		pos_hint: {'left': 1,'top':1}
		#size_hint: 1,1
		Image:
			id: image
			pos_hint: {'top':1}
			size_hint: 1,0.9
			canvas.before:
				Color:
					rgba: 0, 0, 0, 1
				Rectangle:
					pos: self.pos
					size: self.size
			source: 'Ch0_Graphs/Ch0_AVG_7.png'
			mag_filter: 'nearest'
	
		BoxLayout:
			#pos_hint: {'bottom':1}
			size_hint: 1,0.1
			Button:
				text: "1 Week"
				on_press:
					image.source = 'Ch0_Graphs/Ch0_AVG_7.png'
			Button:
				text: "1 Month"
				on_press:
					image.source = 'Ch0_Graphs/Ch0_AVG_31.png'
			Button:
				text: "3 Months"
				on_press:
					image.source = 'Ch0_Graphs/Ch0_AVG_93.png'
			Button:
				text: "6 Months"
				on_press:
					image.source = 'Ch0_Graphs/Ch0_AVG_186.png'
					
<Channel_2>:
	ScreenOne:
		name: "screen_one"
		FloatLayout: #Change back to BoxLayout when the other elements are added
			Button:
				pos_hint: {'right': 1,'center_y': 0.5}
				size_hint: (0.1, 0.2)
				text: "--->"
				on_press:
					root.transition.direction = "left"
					root.transition.duration = 0.5
					root.current = 'screen_two'
	ScreenTwo:
		name: "screen_two"
		FloatLayout: #Change back to BoxLayout when the other elements are added
			Button:
				pos_hint: {'left': 1,'center_y': 0.5}
				size_hint: (0.1, 0.2)
				text: "<---"
				on_press:
					root.transition.direction = "right"
					root.transition.duration = 0.5
					root.current = 'screen_one'
					
