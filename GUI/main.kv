<CustButton@Button>:
	font_size: 32	
	
<PopupBox>:

	oreintation: 'vertical'

	BoxLayout:

		Label:

			text: 'Channel 1'

			font_size: 25

		Label:
			text: 'Channel 2'

			font_size: 25

		Label:

			text: 'Channel 3'
			font_size: 25

		Label:

			text: 'Channel 4'

			font_size: 25

	BoxLayout:

	BoxLayout:

		Button:

			text: 'Run Quick Read'

			on_press: root.quick_read()

		Button:

			text: 'Close'

			on_press: root.popup.dismiss()
			
<CustomPopup>:
	
	id: test
	title: "Title"
	#on_parent: if self.parent == main: self.parent.remove_widget(self)
	size_hint: 0.75, 0.75
	BoxLayout:
		oreintation: 'vertical'

		BoxLayout:

			Label:

				text: 'Channel 1'

				font_size: 25

			Label:
				text: 'Channel 2'

				font_size: 25

			Label:

				text: 'Channel 3'
				font_size: 25

			Label:

				text: 'Channel 4'

				font_size: 25

		BoxLayout:


