				Label:
					text: 'Channel 3'
					font_size: 25

				Label:
					text: 'Channel 4'
					font_size: 25

			BoxLayout:
				Label:
					id: lbl_0
					text: str(root.ch1_val)
					font_size: 25

				Label:
