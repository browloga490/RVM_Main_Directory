						padding: 1,1
						font_size: 20
			
			BoxLayout:

				Button:
					text: 'Run Quick Read'
					on_press: 
						load_pop.open()
						#root.quick_read()
						#root.loading_popup.dismiss()
						root.prog_bar_start()
						load_pop.dismiss()
				
				Button:
					text: 'Close'
					on_press: root.popup.dismiss()
					
